// Future work

Error(r, "Unsupported (reachable) bytecode related primop") :-
  ( op = "addrToAny#"
  ; op = "anyToAddr#"
  ; op = "mkApUpd0#"
  ; op = "newBCO#"
  ; op = "unpackClosure#"
  ; op = "getApStackVal#"
  ),
  Call(r, op, _),
  HasInst(f, r),
  ReachableCode(f).
